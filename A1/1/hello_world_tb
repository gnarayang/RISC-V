#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55a556db3560 .scope module, "hello_world_tb" "hello_world_tb" 2 2;
 .timescale 0 0;
v0x55a556dd3990_0 .var "clk", 0 0;
S_0x55a556db36e0 .scope module, "DUT" "hello_world" 2 4, 3 1 0, S_0x55a556db3560;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clock"
v0x55a556db38b0_0 .net "clock", 0 0, v0x55a556dd3990_0;  1 drivers
E_0x55a556d7a940 .event posedge, v0x55a556db38b0_0;
    .scope S_0x55a556db36e0;
T_0 ;
    %vpi_call 3 7 "$display", "Timestamp " {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x55a556db36e0;
T_1 ;
    %wait E_0x55a556d7a940;
    %vpi_func 3 12 "$time" 64 {0 0 0};
    %pushi/vec4 10, 0, 64;
    %div;
    %addi 1, 0, 64;
    %vpi_call 3 12 "$display", "   %g\011\011 Hello World ", S<0,vec4,u64> {1 0 0};
    %jmp T_1;
    .thread T_1;
    .scope S_0x55a556db3560;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a556dd3990_0, 0, 1;
    %delay 1000, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55a556db3560;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x55a556dd3990_0;
    %inv;
    %store/vec4 v0x55a556dd3990_0, 0, 1;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "hello_world_tb.vl";
    "./hello_world.vl";
